@model MMi_BIS_PA.Models.LoginData
@{
    ViewBag.Title = "CurrentDataPage";
    Layout = "~/Views/Shared/_Layout.cshtml";

}

<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script type=”text/javascript”>

    setInterval(“$(‘#_recentfive’).load(‘CurrentDataPage/UpdateTable’)”,2000);
</script>

<div class="container-fluid overflow-auto" style="height:100%">
    <div class="row">

        @Html.Partial("~/Views/Shared/_NavBar.cshtml")
    </div>
    <div class="row" style="height:inherit">
        <div class="col-1" style="padding-left: 0px;">

        </div>

        <div class="col-10">
            <div class="container-fluid" style="height:100%">
                <div class="row" style="height:150px">
                    <div class="col-3">

                    </div>

                    <div id="barcodeLabel" class="col-6 align-content-center" style="text-align:center">

                        @Html.Partial("_SearchBar")


                    </div>
                    <div class="col-3"></div>

                </div>

                <div id="dataCard" class="row" style="height:85%">
                    @{Html.RenderAction("UpdateTable");}
                </div>

                <div class="row" style="height:50px;"></div>

            </div>
        </div>

        <div class="col-1"></div>

    </div>
</div>










